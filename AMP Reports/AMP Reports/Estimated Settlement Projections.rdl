  <Variables>
    <Variable Name="OrigDebt">
      <Value>=Sum(Fields!OriginalDebt.Value, "ProgramLength")</Value>
    </Variable>
    <Variable Name="ProgramMonths">
      <Value>=Sum(Fields!Program_Months.Value, "ProgramLength")</Value>
    </Variable>
    <Variable Name="TargetSettlement">
      <Value>=Ceiling((Variables!OrigDebt.Value * .6) / Variables!ProgramMonths.Value) * Variables!ProgramMonths.Value</Value>
    </Variable>
