  <DataSets>
    <DataSet Name="ProgramLength">
      <Query>
        <DataSourceName>CREDITSOFT</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ClientID">
            <Value>=Parameters!LeadClientID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>Select OrigDebt_CALCULATED AS 'OriginalDebt', dbo.PSDebtorProgramLength(clientid, 'lead') AS 'Program Months'
from LeadClient
where ClientID = @ClientID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Program_Months">
          <DataField>Program Months</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="OriginalDebt">
          <DataField>OriginalDebt</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
