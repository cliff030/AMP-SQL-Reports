        <DataSourceName>CREDITSOFT</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ClientID">
            <Value>=Parameters!LeadClientID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT SUM(origmonthly) AS 'OrigMonthly'
FROM leadclientcred 
where ClientID = @ClientID
and CreditorID &lt;&gt; 0</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
