        <Field Name="clientssn">
          <DataField>clientssn</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>CREDITSOFT</DataSourceName>
        <CommandText>If @NoLock = 1
Begin
   SET transaction isolation level read uncommitted
End


select	c.clientid, c.wholename, '###-##-'+ right(c.ssn,4) as clientssn
from	clients c
where	c.clientid = @ClientID</CommandText>
        <QueryParameters>
          <QueryParameter Name="@ClientID">
            <Value>=Parameters!ClientID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@NoLock">
            <Value>=Parameters!NoLock.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
